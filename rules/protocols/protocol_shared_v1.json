      "\\bTXA\\b",
      "\\bFFP\\b",
      "\\bfresh\\s+frozen\\s+plasma\\b",
      "\\bpacked\\s+red\\s+blood\\s+cells\\b",
      "\\bPRBC\\b",
      "\\bplatelets?\\b.*\\badministered\\b",
      "\\bcryoprecipitate\\b"
    ],
    "resuscitation_patterns": [
      "\\bMTP\\b",
      "\\bmassive\\s+transfusion\\s+protocol\\b",
      "\\bactivated\\b.*\\bMTP\\b",
