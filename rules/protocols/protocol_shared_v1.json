{
  "meta": {
    "version": "1.0.0",
    "locked": true,
    "description": "Shared pattern buckets for protocol evaluation",
    "design": "Reusable pattern collections for common clinical actions, orders, and assessments"
  },
  "action_buckets": {
    "vital_signs_patterns": [
      "\\bblood\\s+pressure\\b",
      "\\bBP\\b:?\\s*\\d+/\\d+",
      "\\bsystolic\\b.*\\d+\\s*mmHg",
      "\\bheart\\s+rate\\b",
      "\\bHR\\b:?\\s*\\d+",
      "\\bpulse\\b.*\\d+\\s*bpm",
      "\\brespiratory\\s+rate\\b",
      "\\bRR\\b:?\\s*\\d+",
      "\\bGCS\\b",
      "\\bglasgow\\s+coma\\s+scale\\b",
      "\\btemperature\\b.*\\d+"
    ],
    "imaging_order_patterns": [
      "\\bCT\\s+(ordered|performed|scan)\\b",
      "\\bX-ray\\s+(ordered|taken|performed)\\b",
      "\\bultrasound\\b.*\\b(FAST|trauma)\\b",
      "\\bFAST\\s+(exam|examination|scan)\\b",
      "\\bMRI\\s+(ordered|performed)\\b",
      "\\brad(iology)?\\s+(ordered|consult)\\b"
