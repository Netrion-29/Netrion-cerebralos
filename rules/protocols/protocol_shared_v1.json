      "\\bcomplete\\s+blood\\s+count\\b",
      "\\btype\\s+and\\s+(screen|cross)\\b",
      "\\bROTEM\\s+(ordered|performed|panel)\\b",
      "\\blactic\\s+acid\\b",
      "\\blactate\\b.*\\d+",
      "\\bABG\\b",
      "\\barterial\\s+blood\\s+gas\\b",
      "\\bPT/INR\\b",
      "\\bPTT\\b",
      "\\bfibrinogen\\b"
